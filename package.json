{
  "name": "javascript",
  "version": "1.0.0",
  "description": "Pruebas Js-clase",
  "main": "js/index.js",
  "scripts": {
    "start": "npm run dev:start",
    "dev:start": "nodemon --ignore lib --exec babel-node src/server",
    "prod:build": "rimraf lib && babel src -d lib --ignore .test.js",
    "prod:start": "NODE_ENV=production pm2 start lib/server && pm2 logs",
    "prod:stop": "pm2 delete server",
    "prepush": "npm run test && npm run prod:build",
    "test": "eslint src && flow && jest --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore",
    "fix": "npm run lint -- --fix"
  },
  "author": "Jos Andreu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/josandreu/Js-proyect-example.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-flow": "^7.0.0",
    "@jest/core": "^24.8.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-compat": "^3.1.1",
    "eslint-plugin-flowtype": "^3.9.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "flow-bin": "^0.100.0",
    "husky": "^2.4.0",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "pm2": "^3.5.1",
    "rimraf": "^2.6.3"
  },
  "browserslist": [
    "> 1%"
  ],
  "dependencies": {
    "compression": "^1.7.4",
    "express": "^4.17.3"
  }
}